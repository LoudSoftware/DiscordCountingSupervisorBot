{
  "name": "numberlinesupervisor",
  "version": "1.0.0",
  "description": "Number Line supervisor",
  "main": "bot.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/bot.js",
    "watch-node": "nodemon dist/bos.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/bot.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoudSoftware/DiscordCountingSupervisorBot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LoudSoftware/DiscordCountingSupervisorBot/issues"
  },
  "homepage": "https://github.com/LoudSoftware/DiscordCountingSupervisorBot#readme",
  "dependencies": {
    "discord.js": "^11.3.2",
    "dotenv": "^5.0.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^6.0.107",
    "concurrently": "^3.5.1",
    "mocha": "^5.1.1",
    "tsc": "^1.20150623.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0"
  }
}
